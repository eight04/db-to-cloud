{
  "name": "db-to-cloud",
  "version": "0.3.1",
  "description": "Synchronize your database with a cloud drive i.e. Dropbox, Google Drive, OneDrive, Github, etc.",
  "author": "eight04 <eight04@gmail.com>",
  "license": "MIT",
  "keywords": [
    "github",
    "google drive",
    "dropbox",
    "onedrive"
  ],
  "scripts": {
    "lint": "eslint **/*.js --cache",
    "test": "npm run lint && c8 --reporter=lcov mocha -b",
    "preversion": "npm test",
    "version": "npm run build && git add .",
    "postversion": "git push --follow-tags && npm publish",
    "build": "rollup -c"
  },
  "main": "index.js",
  "dependencies": {
    "@eight04/read-write-lock": "^0.1.0",
    "universal-base64": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "c8": "^5.0.4",
    "clipboardy": "^2.1.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.1.0",
    "form-data": "^2.5.1",
    "make-fetch-happen": "^5.0.0",
    "mocha": "^6.2.0",
    "mocha-logger": "^1.0.6",
    "rollup": "^1.21.4",
    "rollup-plugin-alias": "^2.0.0",
    "rollup-plugin-analyzer": "^3.2.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cjs-es": "^0.9.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "sinon": "^7.5.0",
    "tempdir-yaml": "^0.3.0"
  },
  "repository": "eight04/db-to-cloud",
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "files": [
    "lib",
    "dist"
  ],
  "unpkg": "dist/db-to-cloud.min.js"
}
